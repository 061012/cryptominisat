# Config file for the @EXPORT_TYPE@ STP Package
# It defines the following variables
#  CRYPTOMINISAT_INCLUDE_DIRS - include directories for STP
#  CRYPTOMINISAT_LIBRARIES    - libraries to link against
#  CRYPTOMINISAT_EXECUTABLE   - the stp executable

# Compute paths
get_filename_component(CRYPTOMINISAT_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(CRYPTOMINISAT_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

# Our library dependencies (contains definitions for IMPORTED targets)
include("${CRYPTOMINISAT_CMAKE_DIR}/@CRYPTOMINISAT_TARGETS_FILENAME@")

# These are IMPORTED targets created by @CRYPTOMINISAT_TARGETS_FILENAME@
set(CRYPTOMINISAT_LIBRARIES libstp)
set(CRYPTOMINISAT_EXECUTABLE stp)
